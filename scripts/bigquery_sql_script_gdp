CREATE SCHEMA CIS9440_PPP_Loans;

CREATE TABLE CIS9440_PPP_Loans.dim_industry ( 
	industry_id int64 NOT NULL  ,
	industry_classification string  ,
	description string  
 );

ALTER TABLE CIS9440_PPP_Loans.dim_industry ADD PRIMARY KEY ( industry_id )  NOT ENFORCED;

CREATE TABLE CIS9440_PPP_Loans.dim_location ( 
	location_id int64 NOT NULL  ,
	geofips string  ,
	geoname string  ,
	region string  
 );

ALTER TABLE CIS9440_PPP_Loans.dim_location ADD PRIMARY KEY ( location_id )  NOT ENFORCED;

CREATE TABLE CIS9440_PPP_Loans.dim_time ( 
	time_id int64 NOT NULL  ,
	quarter string  ,
	year int64  
 );

ALTER TABLE CIS9440_PPP_Loans.dim_time ADD PRIMARY KEY ( time_id )  NOT ENFORCED;

CREATE TABLE CIS9440_PPP_Loans.facts_gdp ( 
	gdp_id int64 NOT NULL  ,
	unit int64  ,
	location_id int64  ,
	industry_id int64  ,
	time_id int64  
 );

ALTER TABLE CIS9440_PPP_Loans.facts_gdp ADD PRIMARY KEY ( gdp_id )  NOT ENFORCED;

ALTER TABLE CIS9440_PPP_Loans.facts_gdp ADD CONSTRAINT fk_facts_gdp_dim_industry FOREIGN KEY ( industry_id ) REFERENCES CIS9440_PPP_Loans.dim_industry( industry_id ) NOT ENFORCED;

ALTER TABLE CIS9440_PPP_Loans.facts_gdp ADD CONSTRAINT fk_facts_gdp_dim_location FOREIGN KEY ( location_id ) REFERENCES CIS9440_PPP_Loans.dim_location( location_id ) NOT ENFORCED;

ALTER TABLE CIS9440_PPP_Loans.facts_gdp ADD CONSTRAINT fk_facts_gdp_dim_time FOREIGN KEY ( time_id ) REFERENCES CIS9440_PPP_Loans.dim_time( time_id ) NOT ENFORCED;
